class car
{
   int num;
   double gas;
 
 void show(){
	 
    System.out.println("the car number :"+ num);
    System.out.println("the car gas	:"+ gas);
    System.out.print("\n");	
 }
}
 
class PROG2
{
	public static void main(String ages[])
	{
	   car car1 = new car();
	   car1.num = 1234;
	   car1.gas = 20.5;
		
	   car1.show();
	}
}
