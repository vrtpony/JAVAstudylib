//找出最大值
import java.util.Scanner;

class PROG2
{
	public static void main(String args[])
	{
		Scanner sc = new Scanner(System.in);
		
		System.out.print("type to number 1: ");
		int num1 = sc.nextInt();

		System.out.print("type to number 2: ");
		int num2 = sc.nextInt();
		
		int ans = Math.max(num1, num2);
		
		System.out.println("num1 compare num2 = max value: "+ans);
		System.out.println();
		
	}
}
