//java switch .. case... 敘述

import java.util.Scanner;
 
public class sample4
{
    public static void main(String[] arg)
    {
        Scanner sc = new Scanner(System.in);
        System.out.println("Type a number: ");
		System.out.println("\n");		
        int num = sc.nextInt();

		int grade;
		
		grade = num/10;
		
		switch(grade){

			case 10:
			case 9:
				System.out.println("the grades : A ");
				break;

			case 8:			
				System.out.println("the grades : B ");
				break;

			case 7:			
			case 6:
				System.out.println("the grades : C ");
				break;

			case 5:
			case 4:
			case 3:					
			case 2:
			case 1:
			case 0:												
				System.out.println("the grades : D ");
				break;

			default:
				System.out.println("input error !! ");			

		}
		
		System.out.println("\n");
	}
		
}
