import java.util.Scanner;
 
public class PROG2
{
    public static void main(String[] arg)
    {
     int i,j;
     int box[][] ={{1,2,3},{4,5},{6,7,8,9}}; //要加大括號


	for(i=0; i<box.length; i++){
	   System.out.println("box"+"["+(i+1)+"] = "+box[i].length); //呼叫array 長度
	}
      System.out.println("\n");		
     }
		
}
